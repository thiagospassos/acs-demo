@model ApiXResponse
@{
    ViewData["Title"] = "Home Page";
}
<style>
    ul.lorem li {
        display: block;
        float: left;
        border: 1px solid black;
        padding: 15px;
        margin: 3px;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <h2>Response Code: @(Model.Code)</h2>
        <ul class="lorem">
            @if (Model.Values != null)
            {
                foreach (var value in Model.Values)
                {
                    <li>@value</li>
                }
            }
        </ul>
    </div>
</div>
